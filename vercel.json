{
  "version": 2,
  "buildCommand": "rm -f pnpm-lock.yaml && npm install && npm run build",
  "outputDirectory": "dist",
  "installCommand": "rm -f pnpm-lock.yaml && npm install",
  "framework": "vite",
  "env": {
    "NPM_CONFIG_PACKAGE_LOCK": "true"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}